{
  "name": "nookotron",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios-pod-install": "cd ios && pod install",
    "ios-bundle": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/digitalcare100/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
