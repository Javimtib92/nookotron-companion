    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/assets",
    "android-release": "cd android && ./gradlew assembleRelease",
    "android-debug": "cd android && ./gradlew assembleDebug",
    "postinstall": "npx jetify"
  },
